{
	"name": "posedance",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "bun run vue-cli-service serve",
		"build": "bun run css-build && bun run vue-cli-service build",
		"lint": "bun run vue-cli-service lint",
		"css-build": "bun run sass --no-source-map ./src/assets/sass/styles.scss:./public/css/styles.css",
		"css-watch": "bun run css-build -- --watch",
		"start": "bun run css-watch"
	},
	"dependencies": {
		"@tensorflow-models/posenet": "^2.2.2",
		"@tensorflow/tfjs": "^4.12.0",
		"@tensorflow/tfjs-backend-cpu": "^4.12.0",
		"@tensorflow/tfjs-backend-webgl": "^4.12.0",
		"@tensorflow/tfjs-converter": "^4.12.0",
		"@tensorflow/tfjs-core": "^4.12.0",
		"axios": "^1.6.0",
		"bulma": "^0.9.4",
		"core-js": "^3.33.2",
		"install": "^0.13.0",
		"playfab-sdk": "^2.164.241025",
		"vue": "^2.7.15",
		"vue-router": "^3.6.5",
		"vuelidate": "^0.7.7",
		"vuex": "^3.6.2",
		"vuex-persistedstate": "^4.1.0"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~5.0.8",
		"@vue/cli-plugin-eslint": "~5.0.8",
		"@vue/cli-service": "~5.0.8",
		"@babel/eslint-parser": "^7.23.3",
		"eslint": "^8.53.0",
		"eslint-plugin-vue": "^9.18.1",
		"sass": "^1.69.5",
		"sass-loader": "^13.3.2",
		"vue-template-compiler": "^2.7.15"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "@babel/eslint-parser"
		},
		"rules": {}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}